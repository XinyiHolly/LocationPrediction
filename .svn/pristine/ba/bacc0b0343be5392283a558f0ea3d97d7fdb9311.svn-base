package org.tweet;

public class TransitionEx {
	public int first;
	public int second;
	public int destination;
	public int count = 0;
	public float propability = 0;
	
	public TransitionEx(int id1, int id2, int id3)
	{
		first = id1;
		second = id2;
		destination = id3;
	}
	
	int GetFirstPlaceId()
	{
		return first;
	}
	
	int GetSecondPlaceId()
	{
		return second;
	}
	
	int GetDestinationId()
	{
		return destination;
	}
	
}
